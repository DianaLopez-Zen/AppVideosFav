<!DOCTYPE html>
<html lang="en">

<head>
    <meta charset="UTF-8">
    <title>Mis Videos Favoritos</title>
    <%- include ('partials/_header') %>
</head>

<body>

    <!-- HEADER -->
    <%- include ('partials/navigation') %>

        <!-- MAIN CONTENT -->
        <div class="container mt-5">

            <div class="row">

               
                    <div class="container">
                        <div align="center">
                        <h2>Videos Agregados</h2>
                    </div>

                        <ul id="list">
                            <li class="video">

                                <% if (data) { %>
                                    <% for(var i = 0; i < data.length; i++) { %>

                                        <div>
                                            <iframe width="800" height="500" src="<%= link[i].description+'?' %>" frameborder="10" allow="accelerometer;
                                             autoplay; encrypted-media; gyroscope; picture-in-picture" allowfullscreen></iframe>
                                        </div>

                                        <div align="text-center">
                                        <h3>
                                            <%= (data[i].name) %>
                                        </h3>
                                        <p>
                                            <%= (data[i].category) %>
                                        </p>
                                        <p>
                                             
                                          <%= (data[i].date) %>

                                       </p>
                                    
                                       
                                        <div>
                                            
                                            <a href="/update/<%= data[i].id %>" class="btn btn-info">Edit</a>
                                            <a href="/delete/<%= data[i].id %>" class="btn btn-danger">Delete</a>
                                        
                                        </div>
                                        
                                   </div>
                                   <b></br>
                                     <%   } %>
                          <% } %>

                            <form>
                                             <p class="clasificacion">
                                            <input id="radio1" type="radio" name="estrellas" value="5"><!--
                                            --><label for="radio1">★</label><!--
                                            --><input id="radio2" type="radio" name="estrellas" value="4"><!--
                                            --><label for="radio2">★</label><!--
                                            --><input id="radio3" type="radio" name="estrellas" value="3"><!--
                                            --><label for="radio3">★</label><!--
                                            --><input id="radio4" type="radio" name="estrellas" value="2"><!--
                                            --><label for="radio4">★</label><!--
                                            --><input id="radio5" type="radio" name="estrellas" value="1"><!--
                                         --><label for="radio5">★</label>
                                        </p>
                                        </form>
                                        
                            </li>


                        </ul>
                    </div>
                    

            </div>
        </div>


        <!-- section one -->
        <div class="container-fluid" id="about">
            <div class="row bg-dark text-white p-5 text-center">
                <div class="col-sm-6">
                    <i class="fab fa-youtube fa-10x"></i>
                </div>
                <div class="col-sm-6">
                    <h3>The subscribed Youtuber</h3>
                    <p>
                        <span>Elit exercitationem id illo sit distinctio. 
                            .</span>
                    </p>
                </div>
            </div>
        </div>

        <!-- FOOTER -->
        <%- include ('partials/_footer')  %>
